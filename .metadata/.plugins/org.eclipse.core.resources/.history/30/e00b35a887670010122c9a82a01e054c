

public class Array07_Q2_가장시끄러운자리는 {
    public static void main(String[] args) {
        int[][] arr = new int[][] {
        	{2,3,1,4},
        	{8,13,3,33},
        	{7,4,5,80},
        	{17,9,11,5}
        };
        for (int i = 0; i < arr.length; i++) { //4
			for (int j = 0; j < arr[4].length; j++) { //4
				System.out.printf("%2d", arr[i][j]);
								
				
			}
			System.out.println();
			
		}
        

    }
}
