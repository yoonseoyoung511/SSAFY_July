package SWEA;

import java.util.Scanner;

public class Solution_SWEA_2058_자릿수더하기_D1 {
	public static void main(String[] args) {
//		자연수 N은 1부터 9999까지의 자연수이다. (1 ≤ N ≤ 9999)
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt(); // 1 ≤ N ≤ 9999
		
		int sum = 0;
		// 4자리라면?
//		System.out.println(N%10); // 일의자리
//		System.out.println(N/10%10); // 십의자리
//		System.out.println(N/100%10); // 백의자리
//		System.out.println(N/1000%10); // 천의자리
			
		sum = N%10 + N/10%10 + N/100%10 + N/1000%10;
		System.out.println(sum);
		
		
	}
}
